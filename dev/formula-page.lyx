\begin_layout Subsection*
Born desolvation
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Delta G_{born}=\alpha\frac{q^{2}}{r_{ion}\varepsilon}
\]

\end_inset

where 
\begin_inset Formula $\alpha=1,389\ \text{Å}\cdot kJ\cdot mol^{-1}$
\end_inset

, 
\begin_inset Formula $q$
\end_inset

 is the unit charge on the ion and 
\begin_inset Formula $r_{ion}$
\end_inset

 is the radius of the ion in 
\begin_inset Formula $\text{Å}$
\end_inset

.
 
\end_layout

\begin_layout Subsection*
Coulomb interactions
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
U_{coulomb}(I)=\alpha\frac{q_{i}q_{j}}{r_{ij}\varepsilon}e^{-r_{ij}/\kappa}
\]

\end_inset

where 
\begin_inset Formula $\alpha=1,389\ \text{Å}\cdot kJ\cdot mol^{-1}$
\end_inset

, 
\begin_inset Formula $q_{x}$
\end_inset

 is the unit charge on each ion and 
\begin_inset Formula $r_{ij}$
\end_inset

 is the distance between the centers of the ions in 
\begin_inset Formula $\text{Å}$
\end_inset

, and 
\begin_inset Formula 
\[
\kappa\equiv\gamma\sqrt{\frac{\varepsilon T}{I}}
\]

\end_inset

and 
\begin_inset Formula 
\[
\gamma\equiv0.01988\ M^{1/2}\cdot K^{-1/2}\cdot\mbox{\text{\normalfont}Å}.
\]

\end_inset


\end_layout

\begin_layout Subsection*
van der Waals
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
U_{vdw}=\left(\epsilon_{i}\epsilon_{j}\right)^{1/2}\left[\left(\frac{\sigma_{i}+\sigma_{j}}{2r_{ij}}\right)^{12}-2\left(\frac{\sigma_{i}+\sigma_{j}}{2r_{ij}}\right)^{6}\right]
\]

\end_inset

where 
\begin_inset Formula $\sigma_{x}$
\end_inset

 is the van der Waal's radius of an atom and 
\begin_inset Formula $\epsilon_{x}$
\end_inset

 measures the strength of the contribution of atom 
\begin_inset Formula $x$
\end_inset

 to the interaction.
 The values of 
\begin_inset Formula $\sigma_{x}$
\end_inset

 and 
\begin_inset Formula $\epsilon_{x}$
\end_inset

 depend on the types of atoms involved.
 
\begin_inset Formula $r_{ij}$
\end_inset

 is the distance between the centers of the interacting atoms.
 
\end_layout

\begin_layout Subsection*
Hydrogen bonds
\end_layout

\begin_layout Standard
The energy to form a hydrogen bond is:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
U_{hbond}=-4.814\times a\left[\frac{q_{A}q_{H}}{r_{HA}^{2}}cos(\theta)+bc^{-r_{HA}}\right]
\]

\end_inset

where 
\begin_inset Formula $q_{A}$
\end_inset

 and 
\begin_inset Formula $q_{H}$
\end_inset

 are the partial charges on the acceptor and donor, 
\begin_inset Formula $r_{HA}$
\end_inset

 is the distance between the donor proton and acceptor, 
\begin_inset Formula $\theta$
\end_inset

 is the angle between the 
\begin_inset Formula $D-H:A$
\end_inset

 atoms, and 
\begin_inset Formula $a$
\end_inset

, 
\begin_inset Formula $b$
\end_inset

, 
\begin_inset Formula $c$
\end_inset

 are constants that depend on the atoms involved in the interaction.
\end_layout

